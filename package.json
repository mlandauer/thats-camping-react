{
  "name": "thats-camping-react",
  "version": "0.0.0",
  "description": "That's Camping - this time with React",
  "main": "main.js",
  "dependencies": {
    "babel-preset-react": "~6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~6.0.3",
    "express": "^4.4.5",
    "history": "^1.17.0",
    "react": "~0.14.5",
    "react-dom": "~0.14.5",
    "react-router": "^1.0.3",
    "uglify-js": "^2.6.1"
  },
  "devDependencies": {
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "watch": "watchify -d -t [ babelify --presets [ react ] ] main.js -o public/bundle.js -v",
    "browserify": "browserify -t [ babelify --presets [ react ] ] main.js | uglifyjs -c > public/bundle.js",
    "postinstall": "npm run browserify",
    "development": "npm run watch & nodemon server.js",
    "start": "node server.js"
  },
  "author": "Matthew Landauer",
  "license": "GPL"
}
